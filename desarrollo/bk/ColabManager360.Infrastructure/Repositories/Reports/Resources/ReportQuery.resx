  <data name="DailyHoursInput" xml:space="preserve">
    <value>SELECT
p.Id as PeriodId,
p.MaximumDays,
p.MaximumHours, 
c.LegalName,
c.CommercialName,
c3.Name as CountryName,
wa.Name WorkAreaName,
wat.Name WorkAreaTeamName,
at.Description ActivityTypeName,
cs.Description CompanyServiceName,
CONCAT(pi2.LastName1, ' ', pi2.LastName2) as FullLastName,
CONCAT(pi2.FirstName1 , ' ', pi2.FirstName2) as FullFirstName,
a.Description, 
a.DurationHours,
a.TicketCode 
FROM Activities a
INNER JOIN Periods p ON p.Id =a.PeriodId
--INNER JOIN ActivitiesDetail ad on ad.ActivityId=a.Id
INNER JOIN Companies c ON c.Id =a.CompanyId 
INNER JOIN CollaboratorCompanies cc ON cc.CompanyId = c.Id 
INNER JOIN Collaborators c2 ON c2.Id =CC.CollaboratorId 
INNER JOIN PersonInformation pi2 ON pi2.Uid = c2.PersonUid
INNER JOIN Country c3 ON c3.Id=c.Country
INNER JOIN ActivityTypes at ON at.Id=a.ActivityTypeId 
INNER JOIN CompanyServices cs  ON cs.Id =a.CompanyServiceId
LEFT JOIN WorkAreas wa ON wa.Id = C2.WorkAreaId 
LEFT JOIN WorkAreaTeams wat ON wat.WorkAreaId =WA.Id AND WAT.Id =C2.WorkAreaTeamId  

WHERE 
c.Id ={0}
AND p.Year={1}
AND P.Month={2}</value>
